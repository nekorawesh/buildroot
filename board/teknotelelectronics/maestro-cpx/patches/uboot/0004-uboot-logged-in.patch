From dce2a5fabc6251ad54f5ca59b6a40ae764e15de9 Mon Sep 17 00:00:00 2001
From: Mahdi Noori <nekorawesh@gmail.com>
Date: Tue, 23 Jun 2020 14:11:18 +0300
Subject: [PATCH 4/8] uboot: logged in

---
 arch/arm/dts/maestro-cpx-u-boot.dtsi |  1 -
 arch/arm/dts/maestro-cpx.dts         | 17 ++++++++++-------
 2 files changed, 10 insertions(+), 8 deletions(-)

diff --git a/arch/arm/dts/maestro-cpx-u-boot.dtsi b/arch/arm/dts/maestro-cpx-u-boot.dtsi
index 2edad6a26f..47b65809f4 100755
--- a/arch/arm/dts/maestro-cpx-u-boot.dtsi
+++ b/arch/arm/dts/maestro-cpx-u-boot.dtsi
@@ -242,7 +242,6 @@
 };
 
 &usbotg_hs {
-	usb1600;
 	hnp-srp-disable;
 };
 /* USER CODE END addons */
diff --git a/arch/arm/dts/maestro-cpx.dts b/arch/arm/dts/maestro-cpx.dts
index f40ab29ec8..9fcfb3b443 100755
--- a/arch/arm/dts/maestro-cpx.dts
+++ b/arch/arm/dts/maestro-cpx.dts
@@ -75,8 +75,8 @@
 		};
 		/* USER CODE END reserved-memory */
 
-		gpu_reserved: gpu@d4000000 {
-			reg = <0xd4000000 0x4000000>;
+		gpu_reserved: gpu@dc000000 {
+			reg = <0xdc000000 0x4000000>;
 			no-map;
 		};
 	};
@@ -108,7 +108,7 @@
 		blue {
 			label = "alive";
 			gpios = <&gpiof 8 GPIO_ACTIVE_HIGH>;
-			linux,default-trigger = "alive";
+			linux,default-trigger = "heartbeat";
 			default-state = "off";
 		};
 	};
@@ -565,7 +565,8 @@
 	status = "okay";
 
 	/* USER CODE BEGIN ethernet0 */
-	phy-mode = "rmii";
+	local-mac-address = [00 80 ea 42 69 eb];
+	phy-mode = "mii";
 	max-speed = <1000>;
 	phy-handle = <&phy0>;
 
@@ -573,8 +574,9 @@
 		#address-cells = <1>;
 		#size-cells = <0>;
 		compatible = "snps,dwmac-mdio";
-		phy0: ethernet-phy@0 {
-			reg = <0>;
+		phy0: ethernet-phy@1 {
+			device_type = "ethernet-phy";
+			reg = <1>;
 		};
 	};
 	/* USER CODE END ethernet0 */
@@ -900,6 +902,7 @@
 
 &pwr{
 	/* USER CODE BEGIN pwr */
+	pwr-supply = <&vdd>;
 	pwr-regulators {
 		vdd-supply = <&vdd>;
 	};
@@ -949,7 +952,7 @@
 	pinctrl-0 = <&sdmmc2_pins_mx>;
 	pinctrl-1 = <&sdmmc2_opendrain_pins_mx>;
 	pinctrl-2 = <&sdmmc2_sleep_pins_mx>;
-	status = "okay";
+	status = "disabled";
 
 	/* USER CODE BEGIN sdmmc2 */
 	/* USER CODE END sdmmc2 */
