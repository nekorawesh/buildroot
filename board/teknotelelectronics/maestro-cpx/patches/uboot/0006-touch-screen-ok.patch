From 7f8cbfca1f82d4367962e92bcfc2082b27b7f060 Mon Sep 17 00:00:00 2001
From: Mahdi Noori <nekorawesh@gmail.com>
Date: Thu, 25 Jun 2020 16:01:18 +0300
Subject: [PATCH 6/8] touch screen ok

---
 arch/arm/dts/maestro-cpx.dts | 8 ++++----
 1 file changed, 4 insertions(+), 4 deletions(-)

diff --git a/arch/arm/dts/maestro-cpx.dts b/arch/arm/dts/maestro-cpx.dts
index b714cd6ccb..9aaad640db 100755
--- a/arch/arm/dts/maestro-cpx.dts
+++ b/arch/arm/dts/maestro-cpx.dts
@@ -665,8 +665,8 @@
 	touchscreen@2a {
 		compatible = "focaltech,ft6236";
 		reg = <0x2a>;
-		interrupts = <2 2>;
-		interrupt-parent = <&gpiob>;
+		interrupts = <9 2>;
+		interrupt-parent = <&gpioa>;
 		interrupt-controller;
 		touchscreen-size-x = <480>;
 		touchscreen-size-y = <800>;
@@ -676,8 +676,8 @@
 	touchscreen@38 {
 		compatible = "focaltech,ft6336";
 		reg = <0x38>;
-		interrupts = <2 2>;
-		interrupt-parent = <&gpiob>;
+		interrupts = <9 2>;
+		interrupt-parent = <&gpioa>;
 		interrupt-controller;
 		touchscreen-size-x = <480>;
 		touchscreen-size-y = <800>;
